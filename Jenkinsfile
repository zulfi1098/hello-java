
 node  {
	stage('Build') {
		echo "Build"
		sh "ls -la ${pwd()}"
	}
	stage('Test') {
		echo "Test"
		sleep 40000
	}
	stage('Deploy') {
		echo "Deploy"
	}
	stage('PostDeploy') {
		echo "PostDeploy"
	}
	stage('CleanUpResource') {
		echo "CleanUpResource"
	}
	stage('Ping') {
		echo "Deploy"
		echo "ping google.com"
	}
}


